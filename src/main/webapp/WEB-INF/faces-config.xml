<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<application>
		<message-bundle>.org.primefaces.Messages</message-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>signInBean</managed-bean-name>
		<managed-bean-class>beans.SignInBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>signUpBean</managed-bean-name>
		<managed-bean-class>beans.SignUpBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ongoingAuctionsBean</managed-bean-name>
		<managed-bean-class>beans.OngoingAuctionsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>newAuctionBean</managed-bean-name>
		<managed-bean-class>beans.NewAuctionBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>generalBean</managed-bean-name>
		<managed-bean-class>beans.GeneralBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>auctionViewBean</managed-bean-name>
		<managed-bean-class>beans.AuctionViewBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>signIn.xhtml</display-name>
		<from-view-id>/signIn.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>signUp.xhtml</from-outcome>
			<to-view-id>/signUp.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>signIn.xhtml</display-name>
		<from-view-id>/signIn.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ongoingAuctions.xhtml</from-outcome>
			<to-view-id>/ongoingAuctions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>signUp.xhtml</display-name>
		<from-view-id>/signUp.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ongoingAuctions.xhtml</from-outcome>
			<to-view-id>/ongoingAuctions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>signUp.xhtml</display-name>
		<from-view-id>/signUp.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>signIn.xhtml</from-outcome>
			<to-view-id>/signIn.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>myAuctionsHistory.xhtml</display-name>
		<from-view-id>/myAuctionsHistory.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>auctionView.xhtml</from-outcome>
			<to-view-id>/auctionView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>myOngoingAuctions.xhtml</display-name>
		<from-view-id>/myOngoingAuctions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>auctionView.xhtml</from-outcome>
			<to-view-id>/auctionView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auctionView.xhtml</display-name>
		<from-view-id>/auctionView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>myOngoingAuctions.xhtml</from-outcome>
			<to-view-id>/myOngoingAuctions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auctionView.xhtml</display-name>
		<from-view-id>/auctionView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>myAuctionsHistory.xhtml</from-outcome>
			<to-view-id>/myAuctionsHistory.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>myBiddingsHistory.xhtml</display-name>
		<from-view-id>/myBiddingsHistory.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>auctionView.xhtml</from-outcome>
			<to-view-id>/auctionView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auctionView.xhtml</display-name>
		<from-view-id>/auctionView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>myBiddingsHistory.xhtml</from-outcome>
			<to-view-id>/myBiddingsHistory.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>myWinningBids.xhtml</display-name>
		<from-view-id>/myWinningBids.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>auctionView.xhtml</from-outcome>
			<to-view-id>/auctionView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auctionView.xhtml</display-name>
		<from-view-id>/auctionView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>myWinningBids.xhtml</from-outcome>
			<to-view-id>/myWinningBids.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auctionView.xhtml</display-name>
		<from-view-id>/auctionView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>myOngoingBiddings.xhtml</from-outcome>
			<to-view-id>/myOngoingBiddings.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>myOngoingBiddings.xhtml</display-name>
		<from-view-id>/myOngoingBiddings.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>auctionView.xhtml</from-outcome>
			<to-view-id>/auctionView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ongoingAuctions.xhtml</display-name>
		<from-view-id>/ongoingAuctions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>auctionView.xhtml</from-outcome>
			<to-view-id>/auctionView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auctionView.xhtml</display-name>
		<from-view-id>/auctionView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ongoingAuctions.xhtml</from-outcome>
			<to-view-id>/ongoingAuctions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>autoBidOptions.xhtml</display-name>
		<from-view-id>/autoBidOptions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>auctionView.xhtml</from-outcome>
			<to-view-id>/auctionView.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>auctionView.xhtml</display-name>
		<from-view-id>/auctionView.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>autoBidOptions.xhtml</from-outcome>
			<to-view-id>/autoBidOptions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
